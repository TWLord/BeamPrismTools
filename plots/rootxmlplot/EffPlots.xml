    <Canvas nx="2" StyleName="Default" SeriesStyleHint="Variety_7" Name="EffCheck_1D" StyleName="Default" XWidth="600" YWidth="600" PrintName="EffCheck_1D.pdf" >
      <Pad xnd="505" ynd="505" yto="1.35" xto="1.15" ats="0.5" ml="0.175" mt="0.08" mb="0.15" mr="0.15" YTitle="Count" XTitle="Offset (cm)" ds="1.2">

        <Legend Position="TopRight" Width="0.45" Height="0.3" Title="" TextScale="0.5" NColumns="1" />

        <ExternalTH1 rf="Selected.root" tn="EventRates_True" sw="1" do="E1" lc="kBlack" lt="True" />
        <ExternalTH1 rf="EffFriend.root" tn="EventRates_Selected" sw="1" do="E1" lc="Variety_0" lt="Selected" />
        <ExternalTH1 rf="EffFriend.root" tn="EventRates_Corrected" sw="1" do="E1" lc="Variety_2" lt="Corrected" />

      </Pad>
      <Pad xnd="505" ynd="505" yto="1.35" xto="1.15" ats="0.5" ml="0.175" mt="0.08" mb="0.15" mr="0.15" YTitle="Count per stop" XTitle="E_{Avail} (GeV)" ds="1.2">

        <ExternalTH1Array rf="Selected.root" sd="StopEventRates" thn="StopEProxy_True*" do="CHIST" lc="kBlack" />
        <ExternalTH1Array rf="EffFriend.root" sd="StopEventRates" thn="StopEProxy_Selected*" do="E1" lc="Variety_0" />
        <ExternalTH1Array rf="EffFriend.root" sd="StopEventRates" thn="StopEProxy_Corrected*" do="E1" lc="Variety_2" />

      </Pad>
      <Pad xnd="505" ynd="505" yto="1.35" xto="1.15" ats="0.5" ml="0.175" mt="0.08" mb="0.15" mr="0.15" YTitle="Count" XTitle="E (GeV)" ds="1.2">

        <Legend Position="TopRight" Width="0.4" Height="0.3" Title="" TextScale="0.5" NColumns="1" />

        <ExternalTH1Array rf="Selected.root" sd="StopEventRates" thn="StopEProxy_True*" do="CHIST" lc="kBlack" lt="E_{Avail} True" />
        <ExternalTH1Array rf="EffFriend.root" sd="StopEventRates" thn="StopERec_Selected*" do="E1" lc="Variety_0" lt="E_{Rec} Selected" />
        <ExternalTH1Array rf="EffFriend.root" sd="StopEventRates" thn="StopERec_Corrected*" do="E1" lc="Variety_2" lt="E_{Rec} Corrected" />

      </Pad>
      <Pad xnd="505" ynd="505" yto="1.35" xto="1.15" ats="0.5" ml="0.175" mt="0.08" mb="0.15" mr="0.15" YTitle="Corrected/True" XTitle="E_{Avail} (GeV)" dmx="2" dmn="0">

        <Legend Position="TopRight" Width="0.6" Height="0.1" Title="" TextScale="0.3" NColumns="4" />

        <ForSub Replacements="S:6,4,2,0|C:Variety_7,Variety_8,Variety_3,Variety_1" >
          <Hist1DRatio do="P" lt="Stop ${S}" mw="0.4" ms="20" mc="${C}" lc="${C}" sez="true" >
            <ExternalTH1 rf="EffFriend.root" tn="StopEventRates/StopEProxy_Corrected_Stop${S}" />
            <ExternalTH1 rf="Selected.root" tn="StopEventRates/StopEProxy_True_Stop${S}" />
          </Hist1DRatio>
        </ForSub>

      </Pad>
    </Canvas>

